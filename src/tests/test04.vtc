varnishtest "Test invalid incr"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import memcached from "${vmod_topbuild}/src/.libs/libvmod_memcached.so";

	sub vcl_init {
		memcached.servers("--SERVER=localhost:24472");
	}

	sub vcl_deliver {
		set resp.http.count = memcached.incr("test04", 3);
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.count == "-1"
} -run
